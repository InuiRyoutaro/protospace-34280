<div class="card">
<%= link_to prototype_path(prototype.id),method: :get do %>
  <%= image_tag prototype.image, class: 'card__img' if prototype.image.attached? %>
<% end %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
    <%#= prototype.title %> 
    <p class="card__summary">
      <%= prototype.catch_copy %> 
    </p>
    <%# if current_user %>
        <%= link_to prototype.user.name, user_path(prototype.user), class: :card__user%>
    <%# end %>
  </div>

</div>